domReady( function(){
	daum.Music.Config.init( {service_domain:"http://music.daum.net",gaia_domain:"http://bbs.music.daum.net", bi_domain: "https://bill.daum.net" } )
	daum.Music.goToMobile();
	
	var fbEl = daum.$("fb-root");
    if(fbEl) {
        (function(d, s, id) {
          var js, fjs = d.getElementsByTagName(s)[0];
          if (d.getElementById(id)) return;
          js = d.createElement(s); js.id = id;
          js.src = "//connect.facebook.net/ko_KR/all.js#xfbml=1";
          fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    }
}); 
</script>


